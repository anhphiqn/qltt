    <script type="text/javascript">
        function jsdelete(id)
        {
            $.get("<?php echo $this->baseUrl() ?>/master/masterdistrict/confirmdelete/id/"+id,null,function(data){
                if(data>0)
                {
                    bootbox.alert("Hiện có "+data+" Xã phường thuộc quận huyện này. Vui lòng chuyển các Xã Phường trên sang quận huyện khác trước!");
                    return false;
                }
                else 
                {
                    bootbox.confirm("Bạn có chắc muốn xoá quận huyện này không?",
                    function(result){
                        if(result == true)
                        {
                           return window.location = "<?php echo $this->baseUrl() ?>/master/masterdistrict/delete/id/"+id;
                        }
                    });
                }
            });
        }
    </script>
